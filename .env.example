# Unreal Dataset Configuration
# Copy this file to .env and customize as needed

# Server settings
UNREAL_SERVER_HOST=localhost
UNREAL_SERVER_PORT=8080

# Capture settings
UNREAL_CAPTURE_FOV=90.0
UNREAL_CAPTURE_RESOLUTION_WIDTH=640
UNREAL_CAPTURE_RESOLUTION_HEIGHT=640
UNREAL_CAPTURE_SCREENSHOT_TIMEOUT=60
UNREAL_CAPTURE_STABILIZATION_DELAY=1.0

# Output settings
UNREAL_OUTPUT_DIR=./dataset_output
UNREAL_ASSETS_DIR=./assets

# Logging
UNREAL_LOG_LEVEL=INFO
# UNREAL_LOG_FILE=./logs/unreal_dataset.log
